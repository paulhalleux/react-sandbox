{
  "$id": "https://example.com/allof-object.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "AllOf (object)",
  "description": "An example of how to use allOf with JSON Schema.",
  "allOf": [
    {
      "type": "object",
      "properties": {
        "name": {
          "title": "Name",
          "description": "The name of the person.",
          "type": "string"
        }
      },
      "required": [
        "name"
      ]
    },
    {
      "type": "object",
      "properties": {
        "age": {
          "title": "Age",
          "description": "The age of the person.",
          "type": "number"
        }
      },
      "required": [
        "age"
      ]
    }
  ]
}
